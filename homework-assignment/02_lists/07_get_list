def get_list():
    """Continuously asks the user to enter values into a list until they press Enter."""
    lst = []

    while True:
        val = input("â¤ Enter a value to add to the list (or press Enter to finish): ")
        if val == "":  # Exit condition
            break
        lst.append(val)

    return lst

def main():
    lst = get_list()

    if lst:  # Ensure the list is not empty before printing
        print("\nâœ… Here is your final list:", lst)
    else:
        print("\nâš ï¸ No values entered. The list is empty!")

if __name__ == "__main__":
    main()
